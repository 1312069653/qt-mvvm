get_filename_component(MVVM_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
include(CMakeFindDependencyMacro)

list(APPEND CMAKE_MODULE_PATH ${MVVM_CMAKE_DIR})

# Had to use find_package because find_dependency does not support COMPONENTS or MODULE until 3.8.0

find_dependency(Qt5::Widgets REQUIRED)

list(REMOVE_AT CMAKE_MODULE_PATH -1)

